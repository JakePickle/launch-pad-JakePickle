<Window x:Class="LaunchPadJakePickle.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:ViewModels="clr-namespace:LaunchPadJakePickle.ViewModels"
        xmlns:Views="clr-namespace:LaunchPadJakePickle.Views"
        Title="MainWindow" Height="350" Width="525">
    <UserControl>
        <UserControl.Resources>
            <DataTemplate DataType="{x:Type ViewModels:ConsoleViewModel}">
                <Views:ConsoleView/>
            </DataTemplate>
        </UserControl.Resources>
        <Grid>
            <ContentControl x:Name="Console" />
        </Grid>
    </UserControl>
</Window>
